if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    list_inner=[]
    list_outer=[]
    for i in range (x+1):
        for j in range (y+1):
            for m in range (z+1):
                if i+j+m !=n:
                    list_inner=[i,j,m]
                    list_outer.append(list_inner)
print(list_outer)                
                    
            
